{
  "publisher": "dirge",
  "name": "lanhu-image-downloader-ext",
  "displayName": "蓝湖图片下载vscode插件",
  "version": "0.0.1000",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "",
  "author": "xiangwenhu",
  "license": "MIT",
  "funding": "",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiangwenhu/lanhu-image-downloader-ext"
  },
  "bugs": {
    "url": "https://github.com/xiangwenhu/lanhu-image-downloader-ext"
  },
  "sponsor": {
    "url": "https://github.com/xiangwenhu/lanhu-image-downloader-ext"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "webview": {
      "contentSecurityPolicy": "default-src 'none'; style-src 'self'; script-src 'self'"
    },
    "commands": [
      {
        "command": "extension.dirge.open-webview",
        "title": "蓝湖切图下载"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ext-name",
      "properties": {}
    },
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "extension.dirge.open-webview",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup src/index.ts --external vscode",
    "dev": "cross-env NODE_ENV=development nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "async-messenger-js": "^0.0.6",
    "lanhu-image-downloader": "file://D://projects//github-my//lanhu-image-downloader//lanhu-image-downloader-0.0.1012.tgz"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "pnpm": "^10.4.1",
    "reactive-vscode": "^0.2.10",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vscode-ext-gen": "^1.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
